import numpy as np

a = np.array( [ [ [ 1, 2 ], \
                  [ 3, 4 ] ], \
                [ [ 5, 6 ], \
                  [ 7, 8 ] ] ] )

b = np.array( [ [ [ 5, 6 ], \
                  [ 7, 8 ] ], \
                [ [ 1, 2 ], \
                  [ 3, 4 ] ] ] )

print("Stack by hand")

for d in range( 2 ):

    print( a[d] @ b[d] )

print("Stack automatically?")

print( a @ b )
